Property Read Name As String
Property Read Path As String
Property Read Description As String
Property Read Warning As String
Property Read Index As Integer

Private $cArray As Collection
Private $iIndex As Integer

Public Sub _new(cArray As Collection, iIndex As Integer)

  $cArray = cArray

  $iIndex = iIndex

End

Private Function Path_Read() As String

  Return $cArray["Path"]

End

Private Function Name_Read() As String

  Return $cArray["Name"]

End

Private Function Description_Read() As String

  If $cArray.Exist("Description") Then
    Return $cArray["Description"]
  Endif

End

Private Function Warning_Read() As String

  If $cArray.Exist("Warning") Then
    Return $cArray["Warning"]
  Endif

End

Private Function Index_Read() As Integer

  Return $iIndex

End
